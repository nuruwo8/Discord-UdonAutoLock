{
   "name": "discord-udon-auto-lock",
   "version": "2.0.0",
   "description": "Discord Bot for VRChat Udon Auto Lock System",
   "main": ".build/main.js",
   "scripts": {
      "dev": "tsx watch src/main.ts",
      "build": "tsc --noEmit && tsx scripts/build.ts",
      "start": "node .build/main.js",
      "generate-db": "rimraf ./main.db && prisma db push && prisma generate",
      "make-jwt-keys": "openssl genrsa -out ./settings/private/jwt/private_key.pem 2048 && openssl rsa -in ./settings/private/jwt/private_key.pem -pubout -out ./settings/private/jwt/public_key.pem"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/nuruwo8/Discord-UdonAutoLock.git"
   },
   "keywords": [],
   "author": "nuruwo",
   "license": "CC0-1.0",
   "bugs": {
      "url": "https://github.com/nuruwo8/Discord-UdonAutoLock/issues"
   },
   "homepage": "https://github.com/nuruwo8/Discord-UdonAutoLock",
   "devDependencies": {
      "@types/jsonwebtoken": "^9.0.10",
      "@types/log4js": "^2.3.5",
      "@types/node-cron": "^3.0.11",
      "@typescript-eslint/eslint-plugin": "^8.53.0",
      "esbuild": "^0.27.2",
      "eslint": "^9.39.2",
      "eslint-config-prettier": "^10.1.8",
      "eslint-config-standard-with-typescript": "^43.0.1",
      "eslint-plugin-import": "^2.32.0",
      "eslint-plugin-n": "^17.23.2",
      "eslint-plugin-promise": "^7.2.1",
      "prettier": "^3.8.0",
      "prisma": "^6.19.2",
      "rimraf": "^6.1.2",
      "tsx": "^4.21.0",
      "typescript": "^5.9.3"
   },
   "dependencies": {
      "@aws-sdk/client-s3": "^3.848.0",
      "@prisma/client": "^6.19.2",
      "csv-stringify": "^6.6.0",
      "date-fns": "^4.1.0",
      "date-fns-tz": "^3.2.0",
      "discord.js": "^14.25.1",
      "dotenv": "^17.2.3",
      "jsonwebtoken": "^9.0.3",
      "log4js": "^6.9.1",
      "node-cron": "^4.2.1"
   }
}
